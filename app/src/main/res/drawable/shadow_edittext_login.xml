<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- layout background -->
    <item>
        <!-- show rectangle area -->
        <shape android:shape="rectangle" >
            <gradient
                android:angle="27000"
                android:startColor="#E5D8D8"
                android:centerColor="#E5D8D8"
                android:endColor="#E5D8D8"/>
        </shape>
    </item>
    <!-- layout foreground -->
    <item
        android:right="0.5px"
        android:left="0.5px">
        android:top="0.5px">
        <!-- show rectangle area -->
        <shape android:shape="rectangle" >
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>
